{
  "name": "vue3baseextension",
  "displayName": "vue3BaseExtension",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./tsconfig.extension.json --skipLibCheck && vite build && tailwindcss -i ./assets/css/tailwind.css -o ./dist/output.css",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "concurrently \"tsc -w -p ./tsconfig.extension.json --skipLibCheck\" \"vite build --watch\" \"tailwindcss -i ./assets/css/tailwind.css -o ./dist/output.css --watch\"",
    "typecheck": "vue-tsc --noEmit --skipLibCheck"
  },
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vue-3-base-view",
          "title": "Vue 3 Base Extension",
          "icon": "assets/img/default.png"
        }
      ]
    },
    "views": {
      "vue-3-base-view": [
        {
          "type": "webview",
          "id": "base-view-sidebar",
          "name": "Vue 3 Base Extension",
          "icon": "assets/img/default.png",
          "contextualTitle": "Vue 3 Base Extension"
        }
      ]
    }
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@types/node": "^18.11.17",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "concurrently": "^7.6.0",
    "eslint": "^8.26.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4",
    "vite": "^2.9.14",
    "vue-tsc": "^0.40.6"
  }
}
